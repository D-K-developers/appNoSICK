<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nosick3">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logotype"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NOSICK3">
        <activity android:name=".SupportActivity"></activity>
        <activity android:name=".Notification_exp" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EmptyEbolaActivity" />
        <activity android:name=".EmptyCovidActivity" />
        <activity android:name=".LiqwidActivity" />
        <activity android:name=".BirdFluActivity" />
        <activity android:name=".Ebola" />
        <activity android:name=".FluActivity" />
        <activity android:name=".CovidActivity" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>